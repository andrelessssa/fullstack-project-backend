{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Maven: spring-boot:run (mvnw)",
      "type": "shell",
      "command": "mvnw.cmd",
      "args": ["spring-boot:run"],
      "options": {
        "cwd": "${workspaceFolder}/backend"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "reveal": "always",
        "panel": "shared"
      },
      "problemMatcher": []
    }
  ]
}
